<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TRulerX.Phase.AnalysisPage">
    
    <ContentPage Title="정보">
        <Label Text="오류 발생" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" />
    </ContentPage>
    <ContentPage Title="이미지">
        <Label Text="정보를 먼저 입력해 주세요" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" />
    </ContentPage>
    <ContentPage Title="관절">
        <Label Text="사진을 찍거나 선택해 주세요" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" />
    </ContentPage>
    <ContentPage Title="자세">
        <Label Text="먼저 관절을 선택해 주세요" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" />
    </ContentPage>
    <ContentPage Title="결과">
        <Label Text="먼저 자세를 선택해 주세요" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" />
    </ContentPage>
    
</TabbedPage>